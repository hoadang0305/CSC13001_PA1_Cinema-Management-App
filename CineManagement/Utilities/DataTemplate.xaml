<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CineManagement.ViewModels"
                    xmlns:view="clr-namespace:CineManagement.View">
    
    
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VouchersVM}">
        <view:VouchersManagement />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MovieControlViewModel}">
        <view:Movies />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ShowtimeVM}">
        <view:Showtimes />
    </DataTemplate>

</ResourceDictionary>