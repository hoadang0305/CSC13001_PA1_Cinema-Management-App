<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CineManagement.ViewModels"
                    xmlns:view="clr-namespace:CineManagement.View">
    
    
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomerVM}">
        <view:Customer />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <view:Orders />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransactionVM}">
        <view:Transactions />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ServiceVM}">
        <view:Services />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings />
    </DataTemplate>


</ResourceDictionary>